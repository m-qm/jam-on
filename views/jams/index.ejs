

<div class="container-fluid bg-3 text-center">
  <div class="row">
  <h3><%= page %></h3>
  </div>
  <div class="row">
  <% jams.forEach(jam => { %>

  <article class="jam-info col-md-4">
    <p> <%= jam.title %> </p>
    <a href="/jams/<%= jam._id%>">Info</a>
    <p> <%= jam.city %> </p>
    <p> <%= jam.info %> </p>
    <p> <%= jam.description %> </p>
    <p> <%= jam.style %> </p>
    <p> <%= jam.instruments %> </p>




    <form action="jams/<%= jam._id%>/delete" method="post">
      <button class="btn">Delete</button>
    </form>

    <a href="/jams/<%= jam._id%>/edit">Edit</a>
    </article>
  <% }) %>
  </div>
  <div class="row">
  <a class="btn" href="/jams/add">Add a new Jam</a>
  <a class="btn" href="/" class="back-link">Go back</a>
  </div>
</div>